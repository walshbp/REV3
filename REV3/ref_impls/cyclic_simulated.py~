#!/usr/bin/env python

import numpy as np


NS2S = 1.0 / 1000000000.0  

period = 10 #secs
dt = 0.05
mag = 190.0
times = np.arange(0.0, 200.0, dt)


for t in times:
    rot = mag * sin(2.0 * pi * dt / period)
    rate = mag * sin(2.0 * pi * dt / period)
    print "%d,%d,%f,%f,%f" % (t/NS2S,1, 0.0, 0.0, 9.81) # accelerometer inputs
    print "%d,%d,%f,%f,%f" % (t/NS2S,4, 0.0, rate, 0.0) # gyro inputs
